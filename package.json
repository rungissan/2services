{
  "name": "@two-services/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev:serviceA": "npx nx serve serviceA",
    "dev:serviceB": "npx nx serve serviceB",
    "build:all": "npx nx run-many -t build",
    "build:serviceA": "npx nx build serviceA",
    "build:serviceB": "npx nx build serviceB",
    "test:all": "npx nx run-many -t test",
    "test:serviceA": "npx nx test serviceA",
    "test:serviceB": "npx nx test serviceB",
    "lint:all": "npx nx run-many -t lint",
    "docker:build": "npx nx docker-build serviceA && npx nx docker-build serviceB",
    "docker:build:serviceA": "npx nx docker-build serviceA",
    "docker:build:serviceB": "npx nx docker-build serviceB",
    "docker:compose:up": "docker-compose up --build",
    "docker:compose:up:detached": "docker-compose up -d --build",
    "docker:compose:down": "docker-compose down",
    "docker:compose:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:compose:logs": "docker-compose logs -f"
  },
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^8.0.0",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.6.1",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.3",
    "multer": "^2.0.1",
    "redis": "^5.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/eslint": "21.2.3",
    "@nx/eslint-plugin": "21.2.3",
    "@nx/jest": "21.2.3",
    "@nx/js": "21.2.3",
    "@nx/nest": "21.2.3",
    "@nx/node": "21.2.3",
    "@nx/web": "21.2.3",
    "@nx/webpack": "21.2.3",
    "@nx/workspace": "21.2.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.36",
    "@types/jest": "^29.5.12",
    "@types/multer": "^2.0.0",
    "@types/node": "~18.16.9",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nx": "21.2.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "serviceA",
    "serviceA-e2e",
    "serviceB",
    "serviceB-e2e"
  ]
}
